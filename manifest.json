{
    "name": "Pharma Print v1.0",
    "description": "Read and convert prescription to 8cm printable document",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "256": "printer.png"
        },
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "scripting",
        "tabs",
        "notifications"
    ]
}
